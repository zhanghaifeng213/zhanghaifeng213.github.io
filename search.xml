      <url>/2017/04/13/20170413/</url>
      <content type="html"><![CDATA[<pre><code>&apos;&lt;div class=&quot;wrap&quot;&gt;
    &lt;img src=&quot;http://omolotm19.bkt.clouddn.com/0.png&quot; id=&quot;fir&quot;&gt;
    &lt;img src=&quot;http://omolotm19.bkt.clouddn.com/0.png&quot; id=&quot;sec&quot;&gt;
    &lt;img src=&quot;http://omolotm19.bkt.clouddn.com/d.png&quot; &gt;
    &lt;img src=&quot;http://omolotm19.bkt.clouddn.com/0.png&quot; id=&quot;thr&quot;&gt;
    &lt;img src=&quot;http://omolotm19.bkt.clouddn.com/0.png&quot; id=&quot;four&quot;&gt;
    &lt;img src=&quot;http://omolotm19.bkt.clouddn.com/d.png&quot; &gt;
    &lt;img src=&quot;http://omolotm19.bkt.clouddn.com/0.png&quot; id=&quot;five&quot;&gt;
    &lt;img src=&quot;http://omolotm19.bkt.clouddn.com/0.png&quot; id=&quot;six&quot;&gt;
&lt;/div&gt;
&lt;script&gt;
    var oI1=document.getElementById(&quot;fir&quot;),
        oI2=document.getElementById(&quot;sec&quot;),
        oI3=document.getElementById(&quot;thr&quot;),
        oI4=document.getElementById(&quot;four&quot;),
        oI5=document.getElementById(&quot;five&quot;),
        oI6=document.getElementById(&quot;six&quot;);
        setInterval(goudan(),1000);
        function goudan(){
            var time=new Date(),
                hh=time.getHours(),
                mm=time.getMinutes(),
                ss=time.getSeconds(),
                a=Math.floor(hh/10),
                b=Math.floor(hh%10),
                c=Math.floor(mm/10),
                d=Math.floor(mm%10),
                e=Math.floor(ss/10),
                f=Math.floor(ss%10),
                oI=[oI1,oI2,oI3,oI4,oI5,oI6],
                arr=[
                    &quot;http://omolotm19.bkt.clouddn.com/&quot;+a+&quot;.png&quot;,
                    &quot;http://omolotm19.bkt.clouddn.com/&quot;+b+&quot;.png&quot;,
                    &quot;http://omolotm19.bkt.clouddn.com/&quot;+c+&quot;.png&quot;,
                    &quot;http://omolotm19.bkt.clouddn.com/&quot;+d+&quot;.png&quot;,
                    &quot;http://omolotm19.bkt.clouddn.com/&quot;+e+&quot;.png&quot;,
                    &quot;http://omolotm19.bkt.clouddn.com/&quot;+f+&quot;.png&quot;,
                ];
            for(i=0;i&lt;6;i++){
                oI[i].src=&quot;images/&quot;+arr[i]+&quot;.png&quot;;
            };
            return goudan;
        };
&lt;/script&gt;&apos;
</code></pre>]]></content>
      
        
