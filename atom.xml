  <entry>
    <title>demo</title>
    <link href="http://yoursite.com/2017/04/11/20170411/"/>
    <id>http://yoursite.com/2017/04/11/20170411/</id>
    <published>2017-04-10T16:37:57.000Z</published>
    <updated>2017-04-10T16:42:13.419Z</updated>
    
    <content type="html"><![CDATA[<p>‘   &lt;!DOCTYPE html&gt;<br>    <html><br>    <head><br>    <meta charset="utf-8"><br>    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"><br>    <title>Examples</title><br>    <meta name="description" content=""><br>    <meta name="keywords" content=""><br>    <link href="" rel="stylesheet"><br>    <style type="text/css"><br>        *{margin:0;padding:0;}</p>
<pre><code>    #wrap{
        width:200px;
        height:500px;
        margin:0 auto;
    }
    #up,#down{
        color:#fff;
        text-align:center;
        line-height:25px;
        background:#ddd;
    }
    #wrap #box{
        width:200px;
        height:450px;
        overflow:hidden;
    }
    #wrap #list{
        width:200px;
        height:720px;
    }
    #wrap #list img{
        width:200px;
        height:240px;
        list-style:none;
    }
&lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;
    &lt;div id=&quot;wrap&quot;&gt;
        &lt;div id=&quot;up&quot;&gt;向上&lt;/div&gt;
        &lt;div id=&quot;box&quot;&gt;
            &lt;div id=&quot;list&quot;&gt;
                &lt;img src = &quot;http://omolotm19.bkt.clouddn.com/scroll1.png&quot;&gt;
                &lt;img src = &quot;http://omolotm19.bkt.clouddn.com/scroll2.png&quot;&gt;
                &lt;img src = &quot;http://omolotm19.bkt.clouddn.com/scroll3.png&quot;&gt;
            &lt;/div&gt;
        &lt;/div&gt;
        &lt;div id=&quot;down&quot;&gt;向下&lt;/div&gt;
    &lt;/div&gt;
    &lt;script&gt;
        var oWrap = document.getElementById(&quot;wrap&quot;),
            oList = document.getElementById(&quot;list&quot;),
            oUp = document.getElementById(&quot;up&quot;),
            oDown = document.getElementById(&quot;down&quot;),
            index=0,
            time;
            function goudan(b){
                    oList.style.marginTop=index+&quot;px&quot;;
                    if(b){
                        index--;
                        if(index==-270){
                            clearInterval(time);
                            oList.style.marginTop=-270+&quot;px&quot;;
                        };
                    }else{
                        index++;
                        if(index&gt;=0){
                            clearInterval(time);
                            oList.style.marginTop=0+&quot;px&quot;;
                        };
                    }
            }
            oUp.onmousedown=function(){
                time=setInterval(&quot;goudan(true)&quot;,20);
            };
            oDown.onmousedown=function(){
                time=setInterval(&quot;goudan(false)&quot;,20);
            };
            oUp.onmouseup=function(){
               clearInterval(time);
            };
            oDown.onmouseup=function(){
               clearInterval(time);
            };
    &lt;/script&gt;
&lt;/body&gt;
&lt;/html&gt;&apos;
</code></pre></style></head></html></p>]]></content>
    
